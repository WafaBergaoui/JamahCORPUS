    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "12"
  },
  "main": "index.js",
  "dependencies": {
    "firebase": "^8.4.1",
    "firebase-admin": "^9.6.0",
    "firebase-functions": "^3.13.2",
    "firestore": "^1.1.6",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^0.2.0",
    "typescript": "^4.2.4"
  },
  "private": true
}
